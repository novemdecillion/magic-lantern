{
  "scripts": {
    "sass:build": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js src/main/scss --output src/main/resources/static",
    "sass:watch": "nodemon --watch src/main/scss --ext sass,scss --exec 'npm run sass:build'",
    "sass:watch:start": "forever start -c \"npm run sass:watch\" ./",
    "sass:watch:stop": "forever stop ./"
  },
  "dependencies": {
    "bootstrap": "5.0.0-beta2"
  },
  "devDependencies": {
    "forever": "^3.0.4",
    "node-sass": "^5.0.0",
    "node-sass-package-importer": "^5.3.2",
    "nodemon": "^2.0.7"
  }
}
